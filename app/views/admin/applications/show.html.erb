<h4><%= @application.name%>'s Application</h4>
<h5>Pets Applied For</h5>
<% @application.pet_applications.each do |pet_app| %>
<div class="pet_app-<%=pet_app.id%>">
<p><%= pet_app.pet.name %>
  <% if pet_app.status == "Pending" %>
  <%= button_to "Approve", "/admin/applications/#{@application.id}", params: {approve: pet_app.id}, method: :patch %>
  <% end %></p>
</div>
<% end %>
